{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bulma": "^0.6.1",
    "glamor": "^2.20.40",
    "glamorous": "^4.11.0",
    "mobx": "^3.3.2",
    "mobx-react": "^4.3.5",
    "node-sass": "^4.7.2",
    "postcss-cli": "^4.1.1",
    "react": "^16.2.0",
    "react-color": "^2.13.8",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.8.0",
    "tslint-react": "^3.2.0"
  },
  "scripts": {
    "lint": "tslint 'src/**/*.ts?(x)'",
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-build": "node-sass src/_sass/main.scss public/main.css",
    "css-postcss": "postcss --use autoprefixer --output public/main.css public/main.css",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch | react-scripts-ts start",
    "build": "npm run css-deploy && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/node": "^8.0.53",
    "@types/react": "^16.0.25",
    "@types/react-color": "^2.13.3",
    "@types/react-css-modules": "^4.6.0",
    "@types/react-dom": "^16.0.3",
    "@types/react-router-dom": "^4.2.3",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2"
  }
}
